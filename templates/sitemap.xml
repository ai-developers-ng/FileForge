    <url>
        <loc>{{ request.host_url }}terms</loc>
        <lastmod>{{ now.strftime('%Y-%m-%d') }}</lastmod>
        <changefreq>yearly</changefreq>
        <priority>0.5</priority>
    </url>
    <url>
        <loc>{{ request.host_url }}privacy</loc>
